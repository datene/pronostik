<%= render 'dashboard/top_bar' %>
<br><br>
<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
      <div class="panel panel-info" style="background-image: linear-gradient(-180deg, #B5AFAF 0%, #E7E1DD 100%)" >
        <div class="panel-header">
              <div class="nav nav-success inline-block">
                <div class="navbar-header list-inline">
                  <%= render 'dashboard/perso_nav' %>

                </div>
              </div>
          </div>
        <div class="panel-body">
            <div class="bordered card-sean padded-chart">
          <% @queries.each do |que| %>
          <div class="panel">
            <%= link_to question_path(que) do %>
            <div class="panel-heading">
              <h3 class="panel-title"> <%= que.content  %> </h3>
            </div>
            <div class="panel-body"> <ul>

              <% que.scenarios.each do |scen| %>
              <li> <%= scen.content %> </li>
              <% end %>
            </ul>
            <ul>
              <% que.tags.each do |tag| %>
              <div class="label label-info">
                <%= tag.title %>
              </div>
              <% end %>
            </ul>
          </div>
          <% end %>
          <div class="text-muted text-right">
            <%= que.event_date %>
          </div>
          <div class="panel-footer">
            <%= que.category.title %>
          </div>
        </div>
        <% end %>
      </div>
    </div>
  </div>
  </div>

</div>
</div>
<br>
<br>
<br>

<div id="randque-wrapper-dashboard">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <%= render 'dashboard/side_bar', randque: @randque %>
      </div>
    </div>
  </div>
</div>


